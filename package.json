{
  "name": "my-project",
  "version": "1.0.0",
  "description": "Projeto Teste que servir√° de template para outros projetos",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --watchAll --silent --noStackTrace --colors",
    "test:unit": "npm test --  --watchAll -c jest-unit-config.js",
    "test:integration": "npm test -- --watchAll -c jest-integration-config.js",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "prepare": "husky install"
  },
  "keywords": [],
