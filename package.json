    "mongodb-memory-server": "^10.1.4",
    "mongoose": "^8.13.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.0.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.4",
